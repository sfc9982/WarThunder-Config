rangefinderUseThousandth:b = yes

// horizontal range indicators
crosshairHorVertSize:p2 = 3.055,2
crosshair_hor_ranges {
}

// ballistic range indicators
drawUpward:b = yes
distancePos:p2 = 0,0
move:b = yes
radial:b = yes
circleMode:b = yes
crosshairDistHorSizeMain:p2 = 1,0
textPos:p2 = 0,0
textAlign:i = 0
textShift:r = 0.2
drawAdditionalLines:b = yes
crosshairDistHorSizeAdditional:p2 = 0.005,0.003
radialStretch:r = 1
radialAngle:r = 1
radialRadius:p2 = 20,1
drawDistanceCorrection:b = no
distanceCorrectionPos:p2 = 0.0655, 0.260

drawSightMask:b=yes   	

crosshair_distances 
{
}




