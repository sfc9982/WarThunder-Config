// general
crosshairColor:c=0, 225, 0, 255
crosshairLightColor:c=0, 225, 0, 255
thousandth:t = "real"
fontSizeMult:r = 1
lineSizeMult:r = 2.5
applyCorrectionToGun:b = yes
drawCentralLineVert:b = no
drawCentralLineHorz:b = no

// rangefinder
rangefinderHorizontalOffset:r = 114
rangefinderVerticalOffset:r = -30.55
rangefinderProgressBarColor1:c = 0,255,0,0
rangefinderProgressBarColor2:c = 0,0,0,0
